{
    "name": "tiny-pixel/action-network-wp-powertools",
    "type": "wordpress-plugin",
    "license": "MIT",
    "description": "Action Network Utilities for WordPress",
    "homepage": "https://tinypixel.dev",
    "keywords": [
        "wordpress",
        "plugin",
        "boilerplate"
    ],
    "autoload": {
        "psr-4": {
            "TinyPixel\\ActionNetwork\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TinyPixel\\ActionNetwork\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "composer/installers": "^1.0",
        "illuminate/support": "^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/var-dumper": "^4.2",
        "roots/acorn": "dev-master",
        "roots/support": "dev-master"
    },
    "scripts": {
        "lint": "phpcs"
    }
}
