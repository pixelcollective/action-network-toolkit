{
    "name": "tiny-pixel/action-network",
    "type": "wordpress-plugin",
    "license": "MIT",
    "description": "Modular Block Building",
    "homepage": "https://tinypixel.dev",
    "keywords": [
        "wordpress",
        "plugin",
        "boilerplate"
    ],
    "autoload": {
        "psr-4": {
            "ActionNetwork\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActionNetwork\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "composer/installers": "^1.0",
        "roots/acorn": "dev-master",
        "roots/support": "dev-master",
        "illuminate/support": "^5.7",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/console": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/var-dumper": "^4.2"
    },
    "scripts": {
        "lint": "phpcs"
    }
}
